import java.util.ArrayList;

public class Main {
ArrayList<Student> student =new ArrayList<Student>();
Student_Array std =new Student_Array();



	public static void main(String[] args) {
		Main std =new Main();
		
		std.addStudent("이상연",20120198,"01092048533","사학과");
		std.addStudent("송준용", 20120188,"01082032317","사학과");
		std.addStudent("이창수", 20120197,"01020707246","사학과");
		
		System.out.println(std);
	}

	private void addStudent(String name, int id, String phone, String major) {
		student.add(new Student(name,id,phone,major));
		System.out.println(name+"학생 정보 입력 성공");
	}
	
}
