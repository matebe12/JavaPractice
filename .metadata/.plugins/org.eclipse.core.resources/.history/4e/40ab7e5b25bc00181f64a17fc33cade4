import java.util.ArrayList;
import java.util.Iterator;

class Num{
	
	 String name;
	 int phone;
	
	
	Num(String name, int phone){
		this.name=name;
		this.phone=phone;
		
	}
	Num(){
		
	}
	public String toString() {
		return name+""+phone;
		
	}
}

