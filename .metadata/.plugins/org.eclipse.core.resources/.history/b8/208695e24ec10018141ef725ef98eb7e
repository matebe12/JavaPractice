
public class Student {

	private String name;
	private int id;
	private String phone;
	private String major;
	
Student(String name, int id, String phone, String major){
	this.name=name;
	this.id=id;
	this.phone=phone;
	this.major=major;
	
	
	}

	public void updateinfo(int i, String info) {
		
		switch(i) {
		
		case 1: setName(info);// 이름수정
				break;
		case 2:setId(Integer.parseInt(info));//학번 수정
		
		case 3: setPhone(info);
		
			break;
		case 4: setMajor(info);	
				break;
			
		}
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String i) {
		this.phone = i;
	}

	public String getMajor() {
		return major;
	}

	public void setMajor(String major) {
		this.major = major;
	}

	
}
