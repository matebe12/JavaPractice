			System.out.println("d [ :"+"i]"+d.get(i));
		}
		
		d.remove(0);
		/*for(int i=0;i<d.size();i++) {
			System.out.println(d.get(i));
		}*/
		Iterator<Num> it = d.iterator();
		
		while(it.hasNext()) {
			System.out.println(it.next());
		}
		for(Num value : d) {
			System.out.println(value);
		}
		/*while(it.hasNext()) {
